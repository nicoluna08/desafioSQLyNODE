{{#if products}}
<table>
    <thead>
        <th>Titulo</th>
        <th>Precio</th>
        <th>Imagen</th>
    </thead>
    <tbody>
        {{#each products}}
            <tr>
                <td>{{this.title}}</td>
                <td>{{this.price}}</td>
                <td><img  src = "{{this.thumbnail}}" style="width: 100px"/></td>
            </tr>
        {{/each}}
    </tbody>
</table>
{{else}}
<p>No hay productos disponibles</p>
{{/if}}